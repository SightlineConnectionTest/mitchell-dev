 <validationRules>
  <fullName>If_Result_is_Fail_then_must_id_Fail_Step</fullName>
  <active>true</active>
  <description>If the Test Script Result is "Fail" then "Failed Test Step" must have a value.</description>
  <errorConditionFormula>$RecordType.Name = 'Test Case' &amp;&amp; IF( ISPICKVAL(Result__c, "Fail"),  ISBLANK(Failed_Test_Step__c), !ISBLANK(Failed_Test_Step__c))</errorConditionFormula>
  <errorDisplayField>Failed_Test_Step__c</errorDisplayField>
  <errorMessage>If the Result is "Fail", you must lookup the Failed Test Step.</errorMessage>
 </validationRules>
