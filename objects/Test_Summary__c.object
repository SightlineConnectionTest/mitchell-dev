 <fields>
  <fullName>Result__c</fullName>
  <externalId>false</externalId>
  <formula>IF(System_Error__c, "Fail",
IF(AND(Total_Failed_Tests__c = 0, Total_Coverage__c = 0), "Pass", 
IF(AND(Total_Failed_Tests__c = 0, Total_Coverage__c &gt;= 0.75), "Pass", "Fail" ) 
))</formula>
  <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
  <inlineHelpText>The overall test result.  Total Coverage must be 75% or higher and there are no failed test results.</inlineHelpText>
  <label>Result</label>
  <required>false</required>
  <trackHistory>false</trackHistory>
  <trackTrending>false</trackTrending>
  <type>Text</type>
  <unique>false</unique>
 </fields>
 <fields>
  <fullName>Success_Rate__c</fullName>
  <externalId>false</externalId>
  <formula>IF(  Total_Tests_Formula__c  != 0 , (  Total_Tests_Formula__c  -  Total_Failed_Tests_Formula__c  )  /   Total_Tests_Formula__c  , NULL)</formula>
  <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
  <inlineHelpText>The percentage of passing tests.</inlineHelpText>
  <label>Success Rate</label>
  <precision>18</precision>
  <required>false</required>
  <scale>2</scale>
  <trackHistory>false</trackHistory>
  <trackTrending>false</trackTrending>
  <type>Percent</type>
